True  <- root : AddUser Arjun.
True  <- root : NewShare arjunShare reserve(user=Arjun) <= 1000 on rootShare.
True  <- root : GrantUse Arjun on arjunShare.
True  <- Arjun : reserve(user=Arjun) = 300 on arjunShare.
False <- Arjun : reserve(user=Arjun) = 2000 on arjunShare. // exceeds mask

True  <- root : AddUser Andrew.

False <- Arjun : reserve(user=Andrew) = 100 on arjunShare. // no permission on other user
False <- Arjun : reserve(app=443) = 500 on arjunShare. // no permissions on apps in general
True  <- Arjun : reserve(user=Arjun,app=443) = 500 on arjunShare.

True  <- root : AddUser Shriram.

// Arjun : reserve(user=Shriram) = 50. // valid, can be done because Arjun's mask inherited
// Arjun : reserve(user=Shriram) = 5000. // invalid, exceeds Arjun's mask
// Shriram : reserve(user=Shriram) = 5. // invalid, Shriram has no permissions
// root : reserve(user=Shriram) = 2000. // valid, Shriram's reserve is now 2000

// Next line is false because user=Shriram is NOT a subflow of user=Arjun !
False  <- Arjun : NewShare shriramShare reserve(user=Shriram) <= 500 on arjunShare.

// Let's have some fun with capabilities:

True <- Arjun : GrantUse Shriram on arjunShare.
True <- Shriram : reserve(user=Arjun) = 50 on arjunShare.
False <- Shriram : reserve(user=Shriram) = 1 on arjunShare.
